{
  "name": "countly-sdk-web",
  "version": "22.06.4",
  "description": "Countly Web SDK",
  "main": "lib/countly.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "docs": "node_modules/.bin/jsdoc lib/countly.js README.md -c jsdoc_conf.json -d docs",
    "build": "webpack",
    "lint": "./node_modules/.bin/eslint --ignore-path .eslintignore --fix .",
    "lintMD": "markdownlint **/*.md --fix",
    "cypress:open": "cypress open",
    "pretest": "eslint --ignore-path .eslintignore .",
    "test": "./node_modules/.bin/cypress run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Countly/countly-sdk-web"
  },
  "keywords": [
    "Countly",
    "Analytics",
    "Web"
  ],
  "author": "Countly <hello@count.ly> (https://count.ly/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Countly/countly-sdk-web/issues"
  },
  "homepage": "https://count.ly/",
  "devDependencies": {
    "cypress": "^12.0.0",
    "cypress-localstorage-commands": "^1.7.0",
    "docdash": "^1.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "jsdoc": "^3.6.10",
    "markdownlint": "^0.26.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^28.0.0",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.2.0"
  },
  "npmName": "countly-sdk-web",
  "npmFileMap": [
    {
      "basePath": "lib",
      "files": [
        "countly.min.js"
      ]
    }
  ]
